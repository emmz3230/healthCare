generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"
}
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
  bmi   BMI[]
}

model BMI {
  id     Int      @id @default(autoincrement())
  userId Int
  bmi    Int?
  date   DateTime
  user   User     @relation(fields: [userId], references: [id])
}
